{
  "name": "yaedo-genshin",
  "version": "0.1.0",
  "description": "A Genshin Impact Tools with Vue+Electron",
  "main": "build/main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "build": "node scripts/build.js && electron-builder --config scripts/electron-builder.config.js -p always",
    "build:win": "node scripts/build.js && electron-builder --win --config scripts/electron-builder.config.js -p always",
    "build:mac": "node scripts/build.js && electron-builder --mac --config scripts/electron-builder.config.js -p always",
    "build:linux": "node scripts/build.js && electron-builder --linux --config scripts/electron-builder.config.js -p always",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "repository": "https://github.com/Capybara-sea/yaedo-genshin",
  "author": {
    "name": "Capybara-sea",
    "url": "https://github.com/Capybara-sea"
  },
  "publish": [
    {
      "provider": "github",
      "owner": "Capybara-sea",
      "repo": "yaedo-genshin",
      "token": "${{ secrets.GH_TOKEN }}"
    }
  ],
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/lodash-es": "^4.17.7",
    "@types/markdown-it": "^12.2.3",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/runtime-core": "^3.2.47",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.9.0",
    "naive-ui": "^2.34.3",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vfonts": "^0.0.3",
    "vite": "^4.1.4"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.1",
    "pinia": "^2.0.33",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "winston": "^3.8.2"
  }
}