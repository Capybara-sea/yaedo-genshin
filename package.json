{
  "name": "yaedo",
  "version": "0.1.0",
  "description": "A Genshin Impact Tools with Vue+Electron",
  "main": "build/main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "build": "node scripts/build.js && electron-builder",
    "build:win": "node scripts/build.js && electron-builder --win",
    "build:mac": "node scripts/build.js && electron-builder --mac",
    "build:linux": "node scripts/build.js && electron-builder --linux",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "repository": "https://github.com/Capybara-sea/yaedo",
  "author": {
    "name": "Capybara-sea",
    "url": "https://github.com/Capybara-sea"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "electron": "^22.0.2",
    "electron-builder": "^23.6.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.4",
    "typescript": "^4.8.4",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "vue": "^3.2.40"
  }
}